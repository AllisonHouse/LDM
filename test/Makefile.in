# The following rule executes multiple synchronous build processes on a
# remote system for a single make(1) and cc(1) pair.
@MAINTAINER_TRUE@multiCheck:
@MAINTAINER_TRUE@	-$(MAKE) host="$(host)" env="$(env)" make="$(make)" cc="$(cc)" \
@MAINTAINER_TRUE@	    tmp="$(tmp)" configOpts="$$configOpts" singleCheck
@MAINTAINER_TRUE@	-$(MAKE) host="$(host)" env="$(env)" make="$(make)" cc="$(cc)" \
@MAINTAINER_TRUE@	    tmp="$(tmp)" configOpts="--disable-shared $$configOpts" singleCheck

# The following rule executes multiple synchronous build processes on a
# remote system for multiple make(1) and cc(1) pairs.
@MAINTAINER_TRUE@.host.check:
@MAINTAINER_TRUE@	rm -f $*.results $*.log
@MAINTAINER_TRUE@	grep '^[^#]' $< | while IFS=',' read env configOpts make cc tmp; do \
@MAINTAINER_TRUE@	    $(MAKE) host="$*" env="$$env" configOpts="$$configOpts" \
@MAINTAINER_TRUE@		make="$$make" cc="$$cc" tmp="$$tmp" multiCheck </dev/null; \
@MAINTAINER_TRUE@	done >$*.log 2>&1

# The following rule starts concurrent asynchronous builds on multiple remote
# systems.
@MAINTAINER_TRUE@hostchecks:	$(distPath)
@MAINTAINER_TRUE@	-rm -f $@
@MAINTAINER_TRUE@	hosts=`ls *.host | sed 's/.host//'`; \
@MAINTAINER_TRUE@	for host in $$hosts; do \
@MAINTAINER_TRUE@	    $(MAKE) -s $$host.check & pid="$$!"; \
@MAINTAINER_TRUE@	    echo $$host $$pid >>$@; \
@MAINTAINER_TRUE@	    echo "Building on $$host"; \
@MAINTAINER_TRUE@	done; \
@MAINTAINER_TRUE@	status=0; \
@MAINTAINER_TRUE@	while read host pid; do \
@MAINTAINER_TRUE@	    echo "Waiting on $$host"; \
@MAINTAINER_TRUE@	    wait $$pid; \
@MAINTAINER_TRUE@	    stat=$$?; \
@MAINTAINER_TRUE@	    if test $$stat -ne 0; then \
@MAINTAINER_TRUE@		echo 1>&2 \
@MAINTAINER_TRUE@		    "Build on \"$$host\" exited with status $$stat.  " \
@MAINTAINER_TRUE@		    "See file \"$$host.log\"."; \
@MAINTAINER_TRUE@		status=$$(($$status+1)); \
@MAINTAINER_TRUE@	    fi; \
@MAINTAINER_TRUE@	done <$@; \
@MAINTAINER_TRUE@	echo "Done waiting"; \
@MAINTAINER_TRUE@	rm -f $@; \
@MAINTAINER_TRUE@	exit $$status

# hostname,os,cpu,make,cc,config_opts,exit_status
@MAINTAINER_TRUE@results.tab:	redo-results
@MAINTAINER_TRUE@	ls *.results | sed 's/.results//' | \
@MAINTAINER_TRUE@	while read host; do \
@MAINTAINER_TRUE@	    case `ssh -x $$host uname </dev/null` in \
@MAINTAINER_TRUE@		AIX)	os=`ssh -x $$host uname -srv </dev/null`;; \
@MAINTAINER_TRUE@		*)	os=`ssh -x $$host uname -sr </dev/null`;; \
@MAINTAINER_TRUE@	    esac; \
@MAINTAINER_TRUE@	    cpu=`ssh -x $$host uname -m </dev/null`; \
@MAINTAINER_TRUE@	    sed -n "s;^;$$host,$$os,$$cpu,;p" $$host.results </dev/null; \
@MAINTAINER_TRUE@	done >$@

@MAINTAINER_TRUE@portability.csv:	results.tab
@MAINTAINER_TRUE@	@test -e $@ || touch $@
@MAINTAINER_TRUE@	@successes=`grep ',1$$' results.tab | wc -l`; \
@MAINTAINER_TRUE@	failures=`grep ',0$$' results.tab | wc -l`; \
@MAINTAINER_TRUE@	tail -1 $@ | awk -F , '{time=$$1; prevSuccesses=$$2; prevFailures=$$3} \
@MAINTAINER_TRUE@	    END { \
@MAINTAINER_TRUE@		if ('$$successes' != prevSuccesses || \
@MAINTAINER_TRUE@			'$$failures' != prevFailures) { \
@MAINTAINER_TRUE@		    print "CHANGE IN PORTABILITY"; \
@MAINTAINER_TRUE@		    printf("  Successes:    %3d  (was %3d)\n", '$$successes', \
@MAINTAINER_TRUE@			prevSuccesses); \
@MAINTAINER_TRUE@		    printf("  Failures:     %3d  (was %3d)\n", '$$failures', \
@MAINTAINER_TRUE@			prevFailures); \
@MAINTAINER_TRUE@		    total = '$$successes' + '$$failures'; \
@MAINTAINER_TRUE@		    prevTotal = prevSuccesses + prevFailures; \
@MAINTAINER_TRUE@		    printf("  Total:        %3d  (was %3d)\n", total, \
@MAINTAINER_TRUE@			prevTotal); \
@MAINTAINER_TRUE@		    if (total != 0) { \
@MAINTAINER_TRUE@			printf("  Success Rate: %3d%%", \
@MAINTAINER_TRUE@			    int(100*'$$successes'/total)); \
@MAINTAINER_TRUE@		    } \
@MAINTAINER_TRUE@		    if (prevTotal != 0) { \
@MAINTAINER_TRUE@			printf(" (was %3d%)", \
@MAINTAINER_TRUE@			    int(100*prevSuccesses/prevTotal)); \
@MAINTAINER_TRUE@		    } \
@MAINTAINER_TRUE@		    print ""; \
@MAINTAINER_TRUE@		} \
@MAINTAINER_TRUE@	    }'; \
@MAINTAINER_TRUE@	echo `date +%Y%m%d.%H%M`,$$successes,$$failures >>$@

