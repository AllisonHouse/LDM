# The following rule is executed on the remote system.  Setting
# MAKEFLAGS to the empty string on the make(1) invocation line appears
# to be necessary due to a bug in the AIX 5.1 make(1).
@MAINTAINER_TRUE@hostcheck: $(distArchive)
@MAINTAINER_TRUE@	@echo '################################################################'
@MAINTAINER_TRUE@	@echo "Building on `uname -n` with"
@MAINTAINER_TRUE@	@echo "    PATH=$$PATH"
@MAINTAINER_TRUE@	@echo "    CONFIG_OPTS=\"$(CONFIG_OPTS)\""
@MAINTAINER_TRUE@	@echo "    MAKE=\"$(MAKE)\""
@MAINTAINER_TRUE@	@echo "    CC=\"$(CC)\""
@MAINTAINER_TRUE@	@echo
@MAINTAINER_TRUE@	-test -d $(topTmpDir) && rm -rf $(topTmpDir)
@MAINTAINER_TRUE@	mkdir $(topTmpDir)
@MAINTAINER_TRUE@	gunzip -c $(distArchive) | (cd $(topTmpDir) && pax -r)
@MAINTAINER_TRUE@	if cd $(topTmpDir)/$(distId); then \
@MAINTAINER_TRUE@	    if ./configure --prefix=$(topTmpDir) --disable-root-actions \
@MAINTAINER_TRUE@		    $(CONFIG_OPTS) CC='$(CC)' MAKE="$(MAKE)" \
@MAINTAINER_TRUE@		    LDMHOME=$(topTmpDir); then \
@MAINTAINER_TRUE@		MAKEFLAGS= $(MAKE) install uninstall; \
@MAINTAINER_TRUE@		status=$$?; \
@MAINTAINER_TRUE@	    else \
@MAINTAINER_TRUE@		status=$$?; \
@MAINTAINER_TRUE@		cat config.log; \
@MAINTAINER_TRUE@	    fi; \
@MAINTAINER_TRUE@	    cd /tmp; \
@MAINTAINER_TRUE@	    rm -rf $(topTmpDir); \
@MAINTAINER_TRUE@	    exit $$status; \
@MAINTAINER_TRUE@	fi

