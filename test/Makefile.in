@MAINTAINER_TRUE@	ssh -x $(host) chmod +x /tmp/remoteCheck
@MAINTAINER_TRUE@	if ssh -x $(host) /tmp/remoteCheck $(distId) \
@MAINTAINER_TRUE@	    /tmp/$(distName) '"$(env)"' '"$(configOpts)"' '"$(make)"' \
@MAINTAINER_TRUE@		'"$(cc)"' $(tmp); \
@MAINTAINER_TRUE@	then \
@MAINTAINER_TRUE@	    echo "$(env),$(make),$(cc),$(configOpts),1" >>$(host).results; \
@MAINTAINER_TRUE@	else \
@MAINTAINER_TRUE@	    echo "$(env),$(make),$(cc),$(configOpts),0" >>$(host).results; \
